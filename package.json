    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
